// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Books {
  books_id    Int      @id @default(autoincrement())
  books_status  Boolean
  books_price Int   
  vendor_code Int 
  library_name String
}

model Publisher {
  publisher_code    String      @id
  publisher_name  String
  publisher_country String   
  books_id Int
}

model Author {
  author_code    String      @id
  author_name  String
  author_sub String   
  books_id Int
}

model vendor {
  vendor_code    Int      @id
  contact_no  Int
}

model library {
  library_name    String      @id
  library_address  String
  contact_no Int
}

model admin {
  admin_id    Int      @id
  admin_name  String
  contact_no Int
  library_name String
}

model enployee {
  emp_id    Int      @id
  contact_no  Int
}